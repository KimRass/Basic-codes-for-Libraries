    "def solution(phone_number):\n",
    "    length = len(phone_number)\n",
    "    string = \"\"\n",
    "    for idx, num in enumerate(phone_number):\n",
    "        if idx <= length -5:\n",
    "            string += \"*\"\n",
    "        else:\n",
    "            string += num\n",
    "    return string"
   ]
  }

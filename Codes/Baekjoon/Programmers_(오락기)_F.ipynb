    "def solution(name):\n",
    "    nums = [min(ord(char) - ord(\"A\"), ord(\"Z\") - ord(char) + 1) for char in name]\n",
    "    if nums[-1] == 0 or nums[1] == 0:\n",
    "        return sum(nums) + len(nums) - 2\n",
    "    else:\n",
    "        return sum(nums) + len(nums) - 1"
   ]
